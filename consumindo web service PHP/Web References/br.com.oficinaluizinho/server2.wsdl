      <soap:operation soapAction="urn:server.hello#hello" style="rpc" />
      <wsdl:input>
        <soap:body use="encoded" namespace="urn:server.hello" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
      </wsdl:input>
      <wsdl:output>
        <soap:body use="encoded" namespace="urn:server.hello" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" />
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="server.hello">
    <wsdl:port name="server.helloPort" binding="tns:server.helloBinding">
      <soap:address location="http://oficinaluizinho.com.br/php/server2.php" />
    </wsdl:port>
  </wsdl:service>
